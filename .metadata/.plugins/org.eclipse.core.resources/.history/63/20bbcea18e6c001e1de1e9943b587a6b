package com.loops;

public class While {
	public static void main() {
		
		int i = 0 ;
		while (i <= 100) {
			System.out.println("Ini perulangan ke" + i);
			i++;
		}
		
		/*
		 // do while 
		  */
		int j = 0 ;
		do {
			System.out.println("in mah nyoba dulu" + j);
			j++;
		} while(j <= 10);
		
	}
}
